   "source": [
    "from sympy import * "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7393eb42",
   "metadata": {},
   "outputs": [],
   "source": [
    "S, R = symbols(\"S R\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "f25766ae",
   "metadata": {},
   "outputs": [],
   "source": [
    "M = S * (1 - R) ** 2 + (1 - S) * R ** 2\n",
    "P = (S * (1 - R) + (1 - S) * R) ** 2\n",
    "Q = 4 * S * (1 - S)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "6e9bc5d7",
   "metadata": {},
   "outputs": [],
   "source": [
    "result = (M * Q - P).expand()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "543d56f2",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle - \\left(R - S\\right) \\left(8 R S^{2} - 8 R S + R - 4 S^{2} + 3 S\\right)$"
      ],
      "text/plain": [
       "-(R - S)*(8*R*S**2 - 8*R*S + R - 4*S**2 + 3*S)"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "result.simplify().factor()"
   ]
  },
